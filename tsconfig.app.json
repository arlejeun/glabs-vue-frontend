{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["vite.config.*",
  "env.d.ts",
  "src/**/*",
  "**/*/*.d.ts",
  "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": [
      "vite/client",
      "vite-plugin-pages/client",
      "vite-plugin-vue-layouts/client",
      "element-plus/global"
    ]
  }
}
